{
	"compilerOptions": {
		"strict": true,
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"jsx": "preserve",
		"jsxImportSource": "solid-js",
		"isolatedModules": true
	},
	"include": ["**/*.ts", "**/*.tsx", "**.*.js", "**.*.jsx", "vite.config.mjs"]
}
