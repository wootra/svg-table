---
import ExampleLayout from '../../layouts/ExampleLayout.astro';
import Advanced from './(examples)/index.mdx';
const selected = '/advanced-usage'
import {TITLE_TRANSITION} from '../../components/consts';
const canonicalURL = new URL(Astro.url.pathname, Astro.site);
---
<link rel="canonical" href={canonicalURL} />
<ExampleLayout title='Welcome to SVG Title: Advanced Usage' selected={selected}>
	<h1 transition:name="page-title" transition:animate={TITLE_TRANSITION}>Advanced Usage</h1>
	<p>
		SVGTable is not overly complex by nature. Its simiplicity allows
		combining with multiple different SVGs, even SVGTable itself.
	</p>

	<div class='section'>
		<div class='example-container'>
			<Advanced />
		</div>
	</div>
</ExampleLayout>
